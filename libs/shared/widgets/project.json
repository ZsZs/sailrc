{
  "name": "shared-widgets",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/shared/widgets/src",
  "prefix": "sailrc",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/shared/widgets/src/**/*.ts", "libs/shared/widgets/src/**/*.html"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/shared/widgets"],
      "options": {
        "jestConfig": "libs/shared/widgets/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["type:ui", "scope:shared"]
}
