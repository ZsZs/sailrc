{
  "name": "sailrc",
  "version": "0.1.0",
  "license": "MIT",
  "jest-junit": {
    "outputDirectory": "coverage/apps/sail-rc"
  },
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "nx": "nx",
    "config-env": "ts-node ./apps/sail-rc/src/assets/setEnv.ts",
    "start": "npm run config-env -- --environment=dev && ng serve",
    "build": "npm run config-env -- --environment=prod && ng build --configuration=production --base-href https://sailrc.de",
    "build-dev": "npm run config-env -- --environment=dev && nx build --configuration=development",
    "build-test": "npm run config-env -- --environment=test && ng build --configuration=test --base-href https://test.sailrc.de",
    "deploy": "npm run build && firebase deploy",
    "test": "nx run-many --target=test --projects=sail-rc,race-domain,shared-widgets --coverage",
    "lint": "nx workspace-lint && ng lint",
    "sonar": "sonar-scanner",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help"
  },
  "dependencies": {
    "@angular-material-components/file-input": "^8.0.0",
    "@angular/animations": "^14.2.0",
    "@angular/cdk": "^14.2.0",
    "@angular/common": "^14.2.0",
    "@angular/compiler": "^14.2.0",
    "@angular/core": "^14.2.0",
    "@angular/fire": "^7.4.1",
    "@angular/flex-layout": "^14.0.0-beta.40",
    "@angular/forms": "^14.2.0",
    "@angular/google-maps": "^14.2.0",
    "@angular/material": "^14.2.0",
    "@angular/platform-browser": "^14.0.5",
    "@angular/platform-browser-dynamic": "^14.0.5",
    "@angular/router": "^14.2.0",
    "@briebug/ngrx-auto-entity": "^14.0.0-beta.1",
    "@ngrx/effects": "^14.3.0",
    "@ngrx/router-store": "^14.3.0",
    "@ngrx/store": "^14.3.0",
    "@ngrx/store-devtools": "^14.3.0",
    "@nrwl/angular": "14.6.0",
    "angular-cd-timer": "^1.3.0",
    "deep-equal": "^2.0.5",
    "firebase": "^9.9.3",
    "jest-junit": "^14.0.1",
    "ngrx-forms": "^7.0.0",
    "ngx-countdown": "^14.0.0",
    "ngx-logger": "^5.0.11",
    "ngx-mat-timepicker": "^14.0.2",
    "ngx-webcam": "^0.4.1",
    "rxjs": "~7.5.6",
    "ts-luxon": "^3.3.2",
    "tslib": "^2.4.0",
    "xng-breadcrumb": "^8.0.2",
    "xstate": "^4.33.4",
    "zone.js": "^0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.1",
    "@angular-eslint/eslint-plugin": "~14.0.3",
    "@angular-eslint/eslint-plugin-template": "~14.0.3",
    "@angular-eslint/template-parser": "~14.0.3",
    "@angular-extensions/pretty-html-log": "^5.0.1",
    "@angular/cli": "~14.2.1",
    "@angular/compiler-cli": "~14.2.0",
    "@angular/language-service": "~14.2.0",
    "@ngneat/spectator": "^11.1.3",
    "@nrwl/cli": "14.6.0",
    "@nrwl/cypress": "14.6.0",
    "@nrwl/eslint-plugin-nx": "14.6.0",
    "@nrwl/jest": "14.6.0",
    "@nrwl/linter": "14.6.0",
    "@nrwl/nx-cloud": "^14.6.0",
    "@nrwl/tao": "14.6.0",
    "@nrwl/workspace": "14.6.0",
    "@types/jest": "29.0.0",
    "@types/node": "18.7.14",
    "@typescript-eslint/eslint-plugin": "5.36.1",
    "@typescript-eslint/parser": "5.36.1",
    "codelyzer": "^6.0.2",
    "cypress": "^10.7.0",
    "dotenv": "16.0.2",
    "eslint": "8.23.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "jest": "28.1.3",
    "jest-preset-angular": "~12.2.2",
    "ng-packagr": "^14.2.0",
    "ngrx-store-freeze": "^0.2.4",
    "prettier": "2.7.1",
    "sonar-scanner": "^3.1.0",
    "ts-jest": "28.0.8",
    "ts-node": "10.9.1",
    "typescript": "~4.8.2",
    "yargs": "^17.5.1"
  }
}
