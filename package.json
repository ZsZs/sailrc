{
  "name": "sailrc",
  "version": "0.1.0",
  "license": "MIT",
  "jest-junit": {
    "outputDirectory": "coverage/apps/sail-rc"
  },
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "nx": "nx",
    "config-env": "ts-node ./apps/sail-rc/src/assets/setEnv.ts",
    "start": "npm run config-env -- --environment=dev && ng serve",
    "build": "npm run config-env -- --environment=prod && ng build --configuration=production --base-href https://sailrc.de",
    "build-dev": "npm run config-env -- --environment=dev && ng build --configuration=development",
    "build-test": "npm run config-env -- --environment=test && ng build --configuration=test --base-href https://test.sailrc.de",
    "deploy": "npm run build && firebase deploy",
    "test": "nx run-many --target=test --projects=sail-rc,race-domain,shared-widgets --coverage",
    "lint": "nx workspace-lint && ng lint",
    "sonar": "sonar-scanner",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help"
  },
  "dependencies": {
    "@angular-material-components/file-input": "^5.0.1",
    "@angular/animations": "^11.2.0",
    "@angular/cdk": "^11.0.0",
    "@angular/common": "^11.2.0",
    "@angular/compiler": "^11.2.0",
    "@angular/core": "^11.2.0",
    "@angular/fire": "^6.1.5",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "^11.2.0",
    "@angular/google-maps": "^11.2.0",
    "@angular/material": "^11.0",
    "@angular/platform-browser": "^11.2.0",
    "@angular/platform-browser-dynamic": "^11.2.0",
    "@angular/router": "^11.2.0",
    "@briebug/ngrx-auto-entity": "^0.7.0",
    "@ngrx/effects": "^11.1.1",
    "@ngrx/router-store": "^11.1.1",
    "@ngrx/store": "^11.1.1",
    "@ngrx/store-devtools": "^11.1.1",
    "@nrwl/angular": "13.1.2",
    "angular-cd-timer": "^1.1.0",
    "deep-equal": "^1.1.1",
    "firebase": "^8.10.0",
    "jest-junit": "^13.0.0",
    "ngrx-forms": "^6.3.5",
    "ngx-countdown": "^12.0.1",
    "ngx-logger": "^4.3.3",
    "ngx-mat-timepicker": "^11.0.6",
    "ngx-webcam": "^0.3.2",
    "rxjs": "~6.6.3",
    "ts-luxon": "^2.0.3",
    "tslib": "^2.3.1",
    "xng-breadcrumb": "^6.7.0",
    "xstate": "^4.25.0",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.0",
    "@angular-eslint/eslint-plugin": "~1.0.0",
    "@angular-eslint/eslint-plugin-template": "~1.0.0",
    "@angular-eslint/template-parser": "~1.0.0",
    "@angular-extensions/pretty-html-log": "^5.0.1",
    "@angular/cli": "~11.2.0",
    "@angular/compiler-cli": "~11.2.0",
    "@angular/language-service": "~11.2.0",
    "@ngneat/spectator": "^10.0.0",
    "@nrwl/cli": "11.6.3",
    "@nrwl/cypress": "11.6.3",
    "@nrwl/eslint-plugin-nx": "11.6.3",
    "@nrwl/jest": "11.6.3",
    "@nrwl/linter": "11.6.3",
    "@nrwl/nx-cloud": "^11.2.0",
    "@nrwl/tao": "11.6.3",
    "@nrwl/workspace": "11.6.3",
    "@types/jest": "27.4.1",
    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "codelyzer": "^6.0.2",
    "cypress": "^6.9.1",
    "dotenv": "8.2.0",
    "eslint": "7.10.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "jest": "27.5.1",
    "jest-preset-angular": "~11.1.2",
    "ng-packagr": "^11.2.0",
    "ngrx-store-freeze": "^0.2.4",
    "prettier": "2.2.1",
    "sonar-scanner": "^3.1.0",
    "ts-jest": "27.1.4",
    "ts-node": "10.9.1",
    "typescript": "~4.7.2",
    "yargs": "^17.2.1"
  }
}
